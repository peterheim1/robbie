<?xml version="1.0"?>
<robot name="robbie">
        <link name="base_link">
                <visual>
                        <origin xyz="0 0 0.10" rpy="0 0 0" />
                                <geometry>
                                        <box size="0.41 0.34 0.20" />
                                </geometry>
                        <material name="Cyan1">
                                <color rgba="0 1 1 1.0"/>
                        </material>
                </visual>
        </link>
       
        <link name="base_laser">
                <visual>
                        <origin xyz="0 0 .05" rpy="0 0 0" />
                                <geometry>
                                        <cylinder radius="0.015" length="0.10" />
                                </geometry>
                                <material name="black">
                                        <color rgba="0 0 0 1"/>
                                </material>
                </visual>
        </link>
         
        <link name="upper_base_link">
                <visual>
                        <origin xyz="0 0 0.10" rpy="0 0 0" />
                        <geometry>
                                <cylinder radius="0.085" length="0.20" />
                        </geometry>
                        <material name="Cyan2">
                                <color rgba="0 0.5 0.5 0"/>
                        </material>
                </visual>
        </link>
  
        <link name="head_link">
                <visual>
                        <origin xyz="0 0 0.03" rpy="0 0 1.57" />
                        <geometry>
                                <box size="0.13 0.01 0.06" />
                        </geometry>
                        <material name="white">
                                <color rgba="1 1 1 1"/>
                        </material>
                </visual>
        </link>

